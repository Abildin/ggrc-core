{{#if showTooltip}}
  <div
    rel="tooltip"
    data-original-title="{{tooltipContent}}"
  >
    {{#items}}
      <div class="tree-field__item">{{.}}</div>
    {{/items}}
  </div>
{{else}}
  {{#items}}
    <div class="tree-field__item">{{.}}</div>
  {{/items}}
{{/if}}

